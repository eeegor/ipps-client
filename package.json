{
  "name": "ipps-client",
  "version": "1.0.0",
  "description": "A simple frontend for ipps-api",
  "main": "index.js",
  "author": "Egor Kirpichev",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "deploy": "yarn build && now",
    "format": "prettier --write \"{src,test}/**/*.{js,jsx,json,scss}\" \"*.config.{js,json}\" \".babelrc\" \".prettierrc\" \".eslintrc\"",
    "lint": "eslint src && prettier --list-different \"{src,test}/**/*.{js,jsx,scss}\"",
    "prebuild": "rm -rf dist && yarn format && yarn lint && cp -r src/assets dist",
    "start": "webpack-dev-server --mode development",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && jest"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "ajv": "^6.6.1",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-virtualized": "^9.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "classnames": "^2.2.6",
    "css-loader": "^2.0.1",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
